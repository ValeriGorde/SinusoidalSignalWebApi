{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "Console": {
      "IncludeScopes": true
    },
    "File": {
      "IncludeScopes": true,
      "Path": "logs/app.log"
    }
  },
  "AllowedHosts": "*"
}
